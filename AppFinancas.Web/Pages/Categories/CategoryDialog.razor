@page "/category-dialog"
@inherits CategoryDialogBase

<MudDialog>
    <DialogContent>
        <MudText Typo="Typo.h4">@Title:</MudText>
        <MudText Typo="Typo.h6">@Description</MudText>
    </DialogContent>
    <DialogActions>
        <MudButton Variant="Variant.Text" Color="Color.Secondary" OnClick="Close">Fechar</MudButton>
    </DialogActions>
</MudDialog>
